{
  "$ref": "#/definitions/Apis",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Api": {
      "anyOf": [
        {
          "$ref": "#/definitions/HasInterfaceMembers"
        },
        {
          "$ref": "#/definitions/UtilFn"
        },
        {
          "$ref": "#/definitions/Variants"
        }
      ],
      "description": "A documented public API.",
      "markdownDescription": "A documented public API.\n\n- Constructor functions (`kind: \"fn:constructor\"`)\n- Interfaces (`kind: \"interface\"`)"
    },
    "Apis": {
      "additionalProperties": false,
      "properties": {
        "exports": {
          "additionalProperties": {
            "$ref": "#/definitions/Api"
          },
          "description": "The public APIs documented here.",
          "type": "object"
        },
        "links": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "page": {
          "description": "The main API documentation for the entire page.",
          "type": "string"
        }
      },
      "required": ["exports"],
      "type": "object"
    },
    "Const": {
      "additionalProperties": false,
      "properties": {
        "docs": {
          "type": "string"
        },
        "kind": {
          "const": "const",
          "description": "The kind of export.",
          "type": "string"
        },
        "methods": {
          "$ref": "#/definitions/Methods"
        },
        "notes": {
          "type": "string"
        },
        "properties": {
          "$ref": "#/definitions/Properties"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "required": ["kind"],
      "type": "object"
    },
    "ConstructorFn": {
      "additionalProperties": false,
      "description": "A constructor function.",
      "properties": {
        "docs": {
          "type": "string"
        },
        "events": {
          "$ref": "#/definitions/Events"
        },
        "generics": {
          "$ref": "#/definitions/Generics"
        },
        "kind": {
          "const": "fn:constructor",
          "description": "The kind of export.",
          "type": "string"
        },
        "methods": {
          "$ref": "#/definitions/Methods"
        },
        "notes": {
          "type": "string"
        },
        "params": {
          "$ref": "#/definitions/Params"
        },
        "properties": {
          "$ref": "#/definitions/Properties"
        },
        "returns": {
          "$ref": "#/definitions/TypeWithDocs"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "required": ["kind"],
      "type": "object"
    },
    "Events": {
      "additionalProperties": {
        "$ref": "#/definitions/Fn"
      },
      "type": "object"
    },
    "Fn": {
      "additionalProperties": false,
      "properties": {
        "docs": {
          "type": "string"
        },
        "generics": {
          "$ref": "#/definitions/Generics"
        },
        "params": {
          "$ref": "#/definitions/Params"
        },
        "returns": {
          "$ref": "#/definitions/TypeWithDocs"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "Generics": {
      "additionalProperties": {
        "type": "string"
      },
      "type": "object"
    },
    "HasInterfaceMembers": {
      "anyOf": [
        {
          "$ref": "#/definitions/ConstructorFn"
        },
        {
          "$ref": "#/definitions/Interface"
        },
        {
          "$ref": "#/definitions/Const"
        }
      ]
    },
    "Interface": {
      "additionalProperties": false,
      "description": "An interface.",
      "properties": {
        "docs": {
          "type": "string"
        },
        "kind": {
          "const": "interface",
          "description": "The kind of export.",
          "type": "string"
        },
        "methods": {
          "$ref": "#/definitions/Methods"
        },
        "notes": {
          "type": "string"
        },
        "properties": {
          "$ref": "#/definitions/Properties"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "required": ["kind"],
      "type": "object"
    },
    "LonghandProperty": {
      "items": [
        {
          "type": "string"
        },
        {
          "additionalProperties": false,
          "properties": {
            "docs": {
              "type": "string"
            },
            "modifiers": {
              "items": {
                "$ref": "#/definitions/PropertyModifier"
              },
              "type": "array"
            },
            "tag": {
              "$ref": "#/definitions/Tag"
            },
            "tags": {
              "items": {
                "$ref": "#/definitions/Tag"
              },
              "type": "array"
            }
          },
          "type": "object"
        }
      ],
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "Method": {
      "additionalProperties": false,
      "properties": {
        "docs": {
          "type": "string"
        },
        "generics": {
          "$ref": "#/definitions/Generics"
        },
        "params": {
          "$ref": "#/definitions/Params"
        },
        "placement": {
          "enum": ["static", "instance"],
          "type": "string"
        },
        "returns": {
          "$ref": "#/definitions/TypeWithDocs"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "Methods": {
      "additionalProperties": {
        "$ref": "#/definitions/Method"
      },
      "type": "object"
    },
    "Options": {
      "description": "An options argument to a function.",
      "items": [
        {
          "enum": ["@options", "@options?"],
          "type": "string"
        },
        {
          "$ref": "#/definitions/Params"
        }
      ],
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "Params": {
      "additionalProperties": {
        "$ref": "#/definitions/Type"
      },
      "description": "The function's parameters",
      "markdownDescription": "The function's parameters. If the parameter is an options argument, the type is an\narray of `[\"@options\", Params]`.",
      "type": "object"
    },
    "Properties": {
      "additionalProperties": {
        "$ref": "#/definitions/Property"
      },
      "description": "The properties of an object.",
      "examples": [
        ["title"],
        ["title", "string"],
        ["title", "string", "readonly"]
      ],
      "markdownDescription": "The properties of an object.\n\n- With a name only: [\"title\"]\n- With a name and a type: [\"title\", \"string\"]\n- With a name, type and property modifier: [\"title\", \"string\", \"readonly\"]",
      "type": "object"
    },
    "Property": {
      "anyOf": [
        {
          "$ref": "#/definitions/TypeWithDocs"
        },
        {
          "$ref": "#/definitions/PropertyWithModifier"
        },
        {
          "$ref": "#/definitions/LonghandProperty"
        }
      ],
      "description": "A property of an object."
    },
    "PropertyModifier": {
      "const": "readonly",
      "type": "string"
    },
    "PropertyWithModifier": {
      "additionalItems": {
        "$ref": "#/definitions/PropertyModifier"
      },
      "description": "A property with its type and property modifier.\n\n`[\"title\", \"string\", \"readonly\"]`",
      "items": [
        {
          "type": "string"
        },
        {
          "type": "string"
        },
        {
          "$ref": "#/definitions/PropertyModifier"
        }
      ],
      "minItems": 3,
      "type": "array"
    },
    "Tag": {
      "description": "A special category of API.",
      "enum": ["optimization", "renderer", "debug"],
      "markdownDescription": "A special category of API.\n\n- \"optimization\" means that the API is useful for optimizing, but is otherwise not necessary.\n- \"renderer\" means that the API is intended to be used when implementing renderers.\n- \"debug\" means that the API is only available in debug mode.",
      "type": "string"
    },
    "Type": {
      "anyOf": [
        {
          "$ref": "#/definitions/TypeWithDocs"
        },
        {
          "$ref": "#/definitions/Options"
        }
      ],
      "description": "A parameter to a function. If the parameter is an options argument, the type is an array of `[\"@options\", Params]`."
    },
    "TypeWithDocs": {
      "anyOf": [
        {
          "items": {
            "type": "string"
          },
          "maxItems": 2,
          "minItems": 2,
          "type": "array"
        },
        {
          "items": {
            "type": "string"
          },
          "maxItems": 1,
          "minItems": 1,
          "type": "array"
        }
      ],
      "description": "A type with optional documentation.",
      "examples": [["string"], ["string", "a JavaScript string"]],
      "markdownDescription": "A type with optional documentation."
    },
    "UtilFn": {
      "additionalProperties": false,
      "description": "A utility function.",
      "properties": {
        "docs": {
          "type": "string"
        },
        "generics": {
          "$ref": "#/definitions/Generics"
        },
        "kind": {
          "const": "fn:util",
          "description": "The kind of export.",
          "type": "string"
        },
        "notes": {
          "type": "string"
        },
        "params": {
          "$ref": "#/definitions/Params"
        },
        "returns": {
          "$ref": "#/definitions/TypeWithDocs"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        }
      },
      "required": ["kind"],
      "type": "object"
    },
    "Variant": {
      "anyOf": [
        {
          "$ref": "#/definitions/VariantFields"
        },
        {
          "$ref": "#/definitions/VariantWithDocs"
        }
      ]
    },
    "VariantFields": {
      "anyOf": [
        {
          "$ref": "#/definitions/Properties"
        },
        {
          "const": "empty",
          "type": "string"
        }
      ]
    },
    "VariantWithDocs": {
      "description": "An individual variant with documentation.",
      "items": [
        {
          "type": "string"
        },
        {
          "$ref": "#/definitions/VariantFields"
        }
      ],
      "markdownDescription": "An individual variant with documentation.\n\n```json\n[\n  \"a color\",\n  { red: [\"string\"], green: [\"string\"], blue: [\"string\"] },\n]\n```",
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "Variants": {
      "additionalProperties": false,
      "properties": {
        "docs": {
          "type": "string"
        },
        "kind": {
          "const": "variants",
          "description": "The kind of export.",
          "type": "string"
        },
        "notes": {
          "type": "string"
        },
        "tag": {
          "$ref": "#/definitions/Tag"
        },
        "tags": {
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array"
        },
        "variants": {
          "additionalProperties": {
            "$ref": "#/definitions/Variant"
          },
          "type": "object"
        }
      },
      "required": ["kind", "variants"],
      "type": "object"
    }
  }
}
